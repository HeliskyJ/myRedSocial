<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>Filter</filter-name>
        <filter-class>filtro.Filter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>Filter</filter-name>
        <url-pattern>/user*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>SeRegister</servlet-name>
        <servlet-class>servlet.SeRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Log</servlet-name>
        <servlet-class>servlet.Log</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Friendses</servlet-name>
        <servlet-class>servlet.Friendses</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchFriend</servlet-name>
        <servlet-class>servlet.SearchFriend</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FriendReques</servlet-name>
        <servlet-class>servlet.FriendReques</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOut</servlet-name>
        <servlet-class>servlet.LogOut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InPost</servlet-name>
        <servlet-class>servlet.InPost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FriendList</servlet-name>
        <servlet-class>servlet.FriendList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewPost</servlet-name>
        <servlet-class>servlet.NewPost</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SeRegister</servlet-name>
        <url-pattern>/s</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Log</servlet-name>
        <url-pattern>/lo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Friendses</servlet-name>
        <url-pattern>/fri</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchFriend</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FriendReques</servlet-name>
        <url-pattern>/request</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOut</servlet-name>
        <url-pattern>/out</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InPost</servlet-name>
        <url-pattern>/in</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FriendList</servlet-name>
        <url-pattern>/FriendList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewPost</servlet-name>
        <url-pattern>/new</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
